{
  "name": "opendds-performance-dashboard",
  "version": "0.2.0",
  "description": "OpenDSS Performance Dashboard UI",
  "scripts": {
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite",
    "format": "prettier --write '{public,src}/**/*.{css,html,js,svelte,ts}'",
    "lint": "eslint --fix --quiet src --ext .js,.svelte,.ts",
    "reinstall": "rm -rf package-lock.json node_modules; npm install",
    "serve": "vite preview",
    "test": "jest",
    "test:watch": "npm run all -- --watch"
  },
  "contributors": [
    "R. Mark Volkmann",
    "Eldon Ahrold",
    "Timothy Simpson"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenDDS/opendds-performance-dashboard.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@sveltejs/vite-plugin-svelte": "^1.2.0",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/c3": "^0.7.8",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "acorn": "^8.8.1",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^29.3.1",
    "postcss": "^8.4.31",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "stylelint": "^15.6.0",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.8",
    "webpack": "^5.76.0"
  },
  "dependencies": {
    "@types/chartjs": "0.0.31",
    "c3": "^0.7.20",
    "chart.js": "^4.2.1"
  }
}
